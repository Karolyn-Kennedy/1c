Завтраки = Новый СписокЗначений;
Завтраки.Добавить(1,
                  "Каша",
                  Ложь,
                  БибилотекаКартинок.Каша);
                  
Элемент = Завтраки.Добавить();
Элемент.Значение = 2;
Элемент.Представление = "Авокадо тост, бэби";
Элемент.Пометка = Ложь;
Элемент.Картинка = БибилотекаКартинок.АвокадоТост;

МойВыбор = Завтраки.ВыбратьЭлемент();
