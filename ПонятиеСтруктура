Адрес = Новый Структура("Город, Улица, Дом");

Адрес.Город = "Санкт-Петербург";
Адрес.Улица = "Университетская Набережная";
Адрес.Дом = 11;

Сообщить("г. " + Адрес.Город + " ул. " + Адрес.Улица + " д. " + Адрес.Дом);

ФИО = Новый Структура("Фамилия, Имя, Отчество");

ФИО.Фамилия = "Кравченко";
ФИО.Имя = "Анастасия";
ФИО.Отчество = "Александровна";
ФИО.Вставить("Возраст", 23);
