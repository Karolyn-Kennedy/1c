Таб = Новый ТаблицаЗначений;

Колонки = Таб.Колонки;
Колонки.Добавить("Товар");
Колонки.Добавить("Цена");
Колонки.Добавить("Количество");

Строчка = Таб.Добавить();
Строчка.Товар = "Творог";
Строчка.Цена = 50;
Строчка.Количество = 3;

Строчка = Таб.Добавить();
Строчка.Товар = "Молоко";
Строчка.Цена = 65;
Строчка.Количество = 2; 

Строчка = Таб.Добавить();
Строчка.Товар = "Кефир";  
Строчка.Цена = 45;
Строчка.Количество = 4;

ИтоговаяСумма = 0;

Для каждого Строка из Таб Цикл
ТекущийТовар = Строка.Товар;
ТекущаяСумма = Строка.Цена*Строка.Количество;
СтрокаВывода = СтрШаблон( "Товар - %1. Сумма - %2", ТекущийТовар, ТекущаяСумма );
Сообщить(СтрокаВывода);
ИтоговаяСумма = ИтоговаяСумма + ТекущаяСумма ;
КонецЦикла;
